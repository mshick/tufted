{
  "name": "@mshick/tufted",
  "version": "2.0.0-27",
  "keywords": [
    "react",
    "nodejs",
    "nextjs",
    "markdown",
    "mdx",
    "tufte",
    "rehype",
    "remark",
    "sidenote",
    "epigraph"
  ],
  "type": "module",
  "exports": {
    "./remark": "./dist/remark/index.js",
    "./rehype": "./dist/rehype/index.js",
    "./tailwindcss": "./dist/tailwindcss/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "types": "./dist",
  "typesVersions": {
    "*": {
      "remark": [
        "dist/remark/index.d.ts"
      ],
      "rehype": [
        "dist/rehype/index.d.ts"
      ],
      "tailwind": [
        "dist/tailwind/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "del-cli dist",
    "format": "biome check --fix",
    "prepare": "npm run build",
    "release": "np",
    "test": "npm run format && vitest run",
    "typecheck": "tsc --noEmit",
    "watch": "vitest"
  },
  "dependencies": {
    "@shikijs/rehype": "^3.0.0",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "^1.1.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-code-titles": "^1.2.0",
    "rehype-slug": "^6.0.0",
    "remark-directive": "^3.0.1",
    "remark-directive-rehype": "^0.4.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^4.0.1",
    "remark-squeeze-paragraphs": "^6.0.0",
    "remark-unwrap-images": "^4.0.1",
    "tailwindcss": "^4.0.8",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-find-after": "^5.0.0",
    "unist-util-remove": "^4.0.0",
    "unist-util-select": "^5.1.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/hast": "^3.0.4",
    "@types/js-beautify": "^1.14.3",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.14.2",
    "del-cli": "^6.0.0",
    "hastscript": "^9.0.1",
    "js-beautify": "^1.15.3",
    "mdast-util-directive": "^3.1.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "mdast-util-mdxjs-esm": "^2.0.1",
    "np": "^10.2.0",
    "rehype-stringify": "^10.0.1",
    "remark-html": "^16.0.1",
    "remark-mdx": "^3.1.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "to-vfile": "^8.0.0",
    "type-fest": "^4.35.0",
    "typescript": "^5.7.3",
    "vfile": "^6.0.3",
    "vitest": "^3.0.6"
  },
  "packageManager": "npm@10.4.0",
  "engines": {
    "node": "^16.13.0 || ^18.0.0 || ^20.0.0",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
